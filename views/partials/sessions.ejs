<div>
  <div class='bar'>
    Session:
    <select ng-model='sessionId' ng-change='switchSession()'>
      <option ng-repeat='session in sessions'>{{session}}</option>
    </select>
    
    <span><strong ng-bind='clients.length || 0'></strong> clients connected</span>
  
  </div>
</div>

<div class=''>
  <table ng-show='clients.length > 0'>
    <thead>
      <tr>
        <th ng-repeat='(key, value) in clients[0]' ng-bind='key'></th>
      </tr>
    </thead>
    <tbody>
      <tr ng-repeat='client in clients'>
        <td ng-repeat='property in client' ng-bind='property'></td>
      </tr>
    </tbody>
  </table>
</div>

<div class='bottom'>
  <div class='controlwrap'>
    <div class='controls'>
      <button>
        <i class='fa fa-play'></i>
      </button>
      <button>
        <i class='fa fa-stop'></i>
      </button>
    </div>
    
    <span class='song-name'>Ryan Keen - See Me Now</span>
    
    <div class='progress'>
      <div class='current'></div>
    </div>
  </div>

  <div class='console' console>
    <div class='input' contentEditable></div>
  </div>

</div>
