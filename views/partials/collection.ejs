<div class='collection depth-1'>

  <div class='search'>
    <input type='text' placeholder='Search...' 
      ng-model='search'></input>
    <i class='fa fa-search'></i>
  </div>
  
  <table ng-show='models && models.length > 0'>
    <thead>
      <tr>
        <td ng-repeat='(key, value) in models[0]'>
          {{key}}
        </td>
      </tr>
    </thead>
    <tbody>
      <tr ng-repeat='model in models | filter:search'>
        <td ng-repeat='key in model'>
          {{key}}
        </td>
      </tr>
    </tbody>
  </table>

  <div class='message info' ng-hide='models || models.length > 0'>
    <p>No {{name}} avaiable</p>
  </div>

</div>
